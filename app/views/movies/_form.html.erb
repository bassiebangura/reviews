 <%=form_with(model: movie, local:true) do |f|%>
        <%= f.label :title %>
        <%= f.text_field :title, autofocus: true %>
        <%= f.label :rating %>
        <%= f.text_field :rating %>
        <%= f.label :total_gross %>
        <%= f.number_field :total_gross %>
        <%= f.label :description, rows: 10 %>
        <%= f.text_area :description %>
        <%= f.label :released_on %>
        <%= f.date_field :released_on %>
        <%= f.submit %>
<% end %>
